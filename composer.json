	"authors": [
		{
			"name": "James Watts",
			"role": "Author",
			"name": "Rodrigo Iglesias",
			"role": "contributor"
		},
		{
			"name": "CakePHP Community",
			"homepage": "https://github.com/jameswatts/cake-markup-language/graphs/contributors"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "*"
